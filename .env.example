# AWS Configuration
AWS_REGION=us-east-1
STAGE=dev

# OAuth Providers
# Sketchfab
SKETCHFAB_CLIENT_ID=YOUR_CLIENT_ID
SKETCHFAB_CLIENT_SECRET=YOUR_CLIENT_SECRET
SKETCHFAB_REDIRECT_URI=http://localhost:8000/api/oauth/callback/sketchfab

# Google
GOOGLE_CLIENT_ID=YOUR_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_CLIENT_SECRET
GOOGLE_REDIRECT_URI=http://localhost:8000/api/oauth/callback/google

# Twitter
TWITTER_CLIENT_ID=YOUR_CLIENT_ID
TWITTER_CLIENT_SECRET=YOUR_CLIENT_SECRET
TWITTER_REDIRECT_URI=http://localhost:8000/api/oauth/callback/twitter

# Add more providers as needed
# Example: GitHub
# GITHUB_CLIENT_ID=YOUR_CLIENT_ID
# GITHUB_CLIENT_SECRET=YOUR_CLIENT_SECRET
# GITHUB_REDIRECT_URI=http://localhost:8000/api/oauth/callback/github 